net: "model/deploy_deepid.prototxt"

#test_iter: 300
#test_interval: 5000

base_lr: 0.001
momentum: 0.9
weight_decay: 0.0005

lr_policy: "step"
gamma: 0.5
stepsize: 40000
#power: 0.75

display: 100
max_iter: 50000
snapshot: 2000
snapshot_prefix: "result/deepid"
snapshot_diff: true

solver_mode: GPU

